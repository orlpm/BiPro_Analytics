@model BiPro_Analytics.Models.ReporteContagio

@{
    ViewData["Title"] = "Delete";
}


<section id="trabajadores" class="d-flex align-items-cente min-vh-100">
    <div class="container">
        <h1>Borrar</h1>

        <h3>Estas seguros de borrar esto</h3>
        <div>
            <h4>ReporteContagio</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.TipoPrueba)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.TipoPrueba)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.NumeroPruebas)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.NumeroPruebas)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Positivos)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Positivos)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Negativos)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Negativos)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.FechaInicio)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.FechaInicio)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.FechaFin)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.FechaFin)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.FechaRegistro)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.FechaRegistro)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Empresa)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Empresa.Nombre)
                </dd>
            </dl>

            <form asp-action="Delete">
                <input type="hidden" asp-for="Id" />
                <input type="submit" value="Borrar" class="btn btn-danger" /> |
                <a asp-action="Index">Regresar a la lista</a>
            </form>
        </div>
    </div>
</section>