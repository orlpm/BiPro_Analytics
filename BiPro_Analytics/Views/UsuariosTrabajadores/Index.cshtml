@model IEnumerable<BiPro_Analytics.Models.UsuarioTrabajador>

@{
    ViewData["Title"] = "Index";
}

<section id="adminRelaciones" class="d-flex  min-vh-100">
    <div class="container">
        <h1>Index</h1>

        <p>
            <a asp-action="Create">Create New</a>
        </p>
        <table id="tableTransparent" class="table">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.UserId)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.TrabajadorId)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CodigoEmpresa)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.UserId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.TrabajadorId)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CodigoEmpresa)
                        </td>
                        <td>
                            <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                            <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

</div>
</section>